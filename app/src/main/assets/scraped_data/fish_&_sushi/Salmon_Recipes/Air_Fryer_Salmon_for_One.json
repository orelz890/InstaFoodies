{
    "title": "Air Fryer Salmon for One",
    "Category": "Salmon Recipes",
    "Ingredients": [
        {
            "1": "1 (6 ounce) salmon fillet"
        },
        {
            "2": "0.5 teaspoon salt"
        },
        {
            "3": "0.5 teaspoon Greek seasoning (such as Cavender's\u00ae)"
        },
        {
            "4": "0.25 teaspoon ground black pepper"
        },
        {
            "5": "1 pinch dried dill weed"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the air fryer to 370 degrees F (190 degrees C) for 5 minutes."
        },
        {
            "2": "Meanwhile, season salmon fillet with salt, Greek seasoning, pepper, and dill."
        },
        {
            "3": "Line the inner basket of the air fryer with a perforated parchment round. Place salmon onto the parchment, skin side down."
        },
        {
            "4": "Air fry salmon until salmon is cooked through, about 15 minutes."
        }
    ],

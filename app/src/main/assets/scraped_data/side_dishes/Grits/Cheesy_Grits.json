{
    "title": "Cheesy Grits",
    "Category": "Grits",
    "Ingredients": [
        {
            "1": "4 cups water"
        },
        {
            "2": "1 teaspoon salt"
        },
        {
            "3": "1 cup uncooked quick-cooking grits"
        },
        {
            "4": "1 cup shredded Cheddar cheese"
        },
        {
            "5": "0.5 cup shredded Parmesan cheese"
        },
        {
            "6": "2 tablespoons butter"
        },
        {
            "7": "1 ground black pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Bring water and salt to a boil in a saucepan. Whisk in grits, reduce heat to medium-low, and cook until thickened, 5 to 6 minutes. Remove from heat."
        },
        {
            "2": "Stir in Cheddar cheese, Parmesan cheese, and butter until fully combined and cheeses have melted. Season with pepper and serve immediately."
        }
    ],

{
    "title": "Alabama-Style White Barbecue Sauce",
    "Category": "4th of July Recipes",
    "Ingredients": [
        {
            "1": "2 cups mayonnaise"
        },
        {
            "2": "0.5 cup apple cider vinegar"
        },
        {
            "3": "0.25 cup prepared extra-hot horseradish"
        },
        {
            "4": "2 tablespoons fresh lemon juice"
        },
        {
            "5": "2 teaspoons prepared yellow mustard"
        },
        {
            "6": "0.5 teaspoons freshly ground black pepper"
        },
        {
            "7": "1 teaspoon kosher salt"
        },
        {
            "8": "0.5 teaspoon cayenne pepper"
        },
        {
            "9": "0.25 teaspoon garlic powder"
        }
    ],
    "Directions": [
        {
            "1": "Combine mayonnaise, vinegar, horseradish, lemon juice, mustard, black pepper, salt, cayenne pepper, and garlic powder together in a bowl; whisk together thoroughly until creamy and smooth. Cover and refrigerate until ready to use."
        }
    ],

{
    "title": "All Natural Strawberry Lemonade",
    "Category": "4th of July Recipes",
    "Ingredients": [
        {
            "1": "8 large strawberries, halved"
        },
        {
            "2": "2 tablespoons white sugar"
        },
        {
            "3": "7 cups water, divided"
        },
        {
            "4": "2 cups freshly squeezed lemon juice"
        },
        {
            "5": "1 cup white sugar"
        }
    ],
    "Directions": [
        {
            "1": "Place strawberries in a blender; top with 2 tablespoons sugar. Pour 1 cup water over sugared strawberries; blend until completely smooth."
        },
        {
            "2": "Transfer strawberry juice into a large pitcher; stir in remaining 6 cups water, lemon juice, and 1 cup sugar until well blended. Chill before serving."
        }
    ],

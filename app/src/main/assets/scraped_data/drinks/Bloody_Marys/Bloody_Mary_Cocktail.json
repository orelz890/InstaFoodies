{
    "title": "Bloody Mary Cocktail",
    "Category": "Bloody Marys",
    "Ingredients": [
        {
            "1": "0.5 cups ice cubes"
        },
        {
            "2": "4 fluid ounces tomato juice"
        },
        {
            "3": "0.5 fluid ounces vodka"
        },
        {
            "4": "0.25 fluid ounce fresh lemon juice"
        },
        {
            "5": "4 dashes hot pepper sauce (such as Tabasco\u00ae)"
        },
        {
            "6": "2 dashes Worcestershire sauce"
        },
        {
            "7": "1 pinch salt and ground black pepper"
        },
        {
            "8": "1 ice"
        },
        {
            "9": "1 stalk celery, for garnish"
        }
    ],
    "Directions": [
        {
            "1": "Fill a pint glass or goblet with 1 1/2 cups ice. Set aside."
        },
        {
            "2": "Combine tomato juice, vodka, lemon juice, hot pepper sauce, Worcestershire sauce, salt, black pepper, and 1 cup ice in a mixing glass. Stir until chilled and strain into ice-filled pint glass or goblet."
        },
        {
            "3": "Garnish with a celery stalk."
        }
    ],

{
    "title": "Blackened Seasoning Mix",
    "Category": "Whole30 Recipes",
    "Ingredients": [
        {
            "1": "0.5 tablespoons paprika"
        },
        {
            "2": "1 tablespoon garlic powder"
        },
        {
            "3": "1 tablespoon onion powder"
        },
        {
            "4": "1 tablespoon ground dried thyme"
        },
        {
            "5": "1 teaspoon ground black pepper"
        },
        {
            "6": "1 teaspoon cayenne pepper"
        },
        {
            "7": "1 teaspoon dried basil"
        },
        {
            "8": "1 teaspoon dried oregano"
        }
    ],
    "Directions": [
        {
            "1": "Stir together paprika, garlic powder, onion powder, thyme, black pepper, cayenne pepper, basil, and oregano in a bowl until evenly mixed."
        },
        {
            "2": "Store in an airtight container in a cool, dry place."
        }
    ],

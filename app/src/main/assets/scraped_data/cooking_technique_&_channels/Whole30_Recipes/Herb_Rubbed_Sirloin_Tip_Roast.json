{
    "title": "Herb Rubbed Sirloin Tip Roast",
    "Category": "Whole30 Recipes",
    "Ingredients": [
        {
            "1": "0.25 tablespoons paprika"
        },
        {
            "2": "1 tablespoon kosher salt"
        },
        {
            "3": "1 teaspoon garlic powder"
        },
        {
            "4": "0.5 teaspoon ground black pepper"
        },
        {
            "5": "0.5 teaspoon onion powder"
        },
        {
            "6": "0.5 teaspoon ground cayenne pepper"
        },
        {
            "7": "0.5 teaspoon dried oregano"
        },
        {
            "8": "0.5 teaspoon dried thyme"
        },
        {
            "9": "2 tablespoons olive oil"
        },
        {
            "10": "1 (3 pound) sirloin tip roast"
        }
    ],
    "Directions": [
        {
            "1": "Mix paprika, kosher salt, garlic powder, black pepper, onion powder, cayenne pepper, oregano, and thyme in a small bowl. Stir in olive oil; allow mixture to sit for about 15 minutes."
        },
        {
            "2": "Preheat the oven to 350 degrees F (175 degrees C). Line a baking sheet with aluminum foil."
        },
        {
            "3": "Place roast on the prepared baking sheet. Rub spice mixture all over roast."
        },
        {
            "4": "Roast in the preheated oven until an instant-read thermometer inserted into the center reads at least 145 degrees F (63 degrees C), about 1 hour. Let sit 15 minutes before slicing."
        }
    ],

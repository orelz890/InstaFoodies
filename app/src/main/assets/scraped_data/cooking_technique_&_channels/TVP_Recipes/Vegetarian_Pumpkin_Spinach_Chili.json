{
    "title": "Vegetarian Pumpkin Spinach Chili",
    "Category": "TVP Recipes",
    "Ingredients": [
        {
            "1": "1 (28 ounce) can diced tomatoes"
        },
        {
            "2": "1 (14 ounce) can 100% pure pumpkin"
        },
        {
            "3": "1 cup vegetable juice"
        },
        {
            "4": "1 cup chopped okra"
        },
        {
            "5": "1 cup chopped broccoli"
        },
        {
            "6": "1  carrot, peeled and chopped"
        },
        {
            "7": "1 small zucchini, diced"
        },
        {
            "8": "1 small onion, diced"
        },
        {
            "9": "2 tablespoons pumpkin pie spice"
        },
        {
            "10": "2 tablespoons white sugar"
        },
        {
            "11": "2 tablespoons white vinegar"
        },
        {
            "12": "1 teaspoon chili powder, or to taste"
        },
        {
            "13": "1 teaspoon salt"
        },
        {
            "14": "0.5 teaspoon ground black pepper"
        },
        {
            "15": "1 (12 ounce) package vegetarian ground beef crumbles"
        },
        {
            "16": "1 (19 ounce) can fava beans, drained"
        },
        {
            "17": "2 cups chopped spinach"
        }
    ],
    "Directions": [
        {
            "1": "Combine the tomatoes, pumpkin, vegetable juice, okra, broccoli, carrot, zucchini, onion, pumpkin pie spice, sugar, vinegar, chili powder, salt, and pepper in a slow cooker; cook on High until the vegetables are tender, 3 to 4 hours."
        },
        {
            "2": "Stir the vegetarian ground beef crumbles, fava beans, and spinach into the tomato mixture; continue cooking until completely warmed, 20 to 30 minutes more."
        }
    ],
    "Details": [

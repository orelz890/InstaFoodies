{
    "title": "Granny's Easy Chili",
    "Category": "TVP Recipes",
    "Ingredients": [
        {
            "1": "1  onion, chopped"
        },
        {
            "2": "5 cloves crushed garlic"
        },
        {
            "3": "1 stalk celery, chopped"
        },
        {
            "4": "1 tablespoon olive oil"
        },
        {
            "5": "8 ounces textured vegetable protein"
        },
        {
            "6": "4 (16 ounce) cans chili beans"
        },
        {
            "7": "1 (14.5 ounce) can stewed tomatoes"
        },
        {
            "8": "4 cups water"
        },
        {
            "9": "2 tablespoons brown sugar"
        },
        {
            "10": "1 salt and pepper to taste"
        },
        {
            "11": "2 teaspoons chili powder"
        }
    ],
    "Directions": [
        {
            "1": "In a large saucepan saute onion, garlic, celery and olive oil until onions are soft."
        },
        {
            "2": "Add to saucepan textured vegetable protein, chili beans, tomatoes, water, sugar, salt and pepper, and chili powder. Stir occasionally, let simmer for 30 minutes. The textured vegetable protein will expand when added to liquid."
        }
    ],

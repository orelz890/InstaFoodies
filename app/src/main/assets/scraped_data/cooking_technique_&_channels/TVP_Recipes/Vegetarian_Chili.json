    "title": "Vegetarian Chili",
    "Category": "TVP Recipes",
    "Ingredients": [
        {
            "1": "1 (12 ounce) package frozen burger-style crumbles"
        },
        {
            "2": "2 (15 ounce) cans black beans, rinsed and drained"
        },
        {
            "3": "2 (15 ounce) cans dark red kidney beans"
        },
        {
            "4": "1 (15 ounce) can light red kidney beans"
        },
        {
            "5": "1 (29 ounce) can diced tomatoes"
        },
        {
            "6": "1 (12 fluid ounce) can tomato juice"
        },
        {
            "7": "5  onions, chopped"
        },
        {
            "8": "3 tablespoons chili powder"
        },
        {
            "9": "0.5 tablespoons ground cumin"
        },
        {
            "10": "1 tablespoon garlic powder"
        },
        {
            "11": "2  bay leaves"
        },
        {
            "12": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "In a large pot, combine meat substitute, black beans, kidney beans, diced tomatoes, tomato juice, onions, chili powder, cumin, garlic powder, bay leaves, salt and pepper. Bring to a simmer and cover. Let the chili simmer for at least 1 hour before serving."
        }

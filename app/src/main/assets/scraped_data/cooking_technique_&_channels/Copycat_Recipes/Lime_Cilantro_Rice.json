{
    "title": "Lime Cilantro Rice",
    "Category": "Copycat Recipes",
    "Ingredients": [
        {
            "1": "2 cups water"
        },
        {
            "2": "1 cup long-grain white rice"
        },
        {
            "3": "1 tablespoon butter"
        },
        {
            "4": "0.5 cup chopped cilantro"
        },
        {
            "5": "2 tablespoons fresh lime juice"
        },
        {
            "6": "1 teaspoon lime zest"
        }
    ],
    "Directions": [
        {
            "1": "Bring water to a boil in a small saucepan. Stir in rice and butter. Reduce heat to low, cover, and simmer until rice is tender, about 20 minutes."
        },
        {
            "2": "Just before serving, add cilantro, lime juice, and lime zest."
        }
    ],

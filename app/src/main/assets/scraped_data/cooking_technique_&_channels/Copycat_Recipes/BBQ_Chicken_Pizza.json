{
    "title": "BBQ Chicken Pizza",
    "Category": "Copycat Recipes",
    "Ingredients": [
        {
            "1": "1 (12 inch) pre-baked pizza crust"
        },
        {
            "2": "1 cup spicy barbeque sauce"
        },
        {
            "3": "2  skinless boneless chicken breast halves, cooked and cubed"
        },
        {
            "4": "1 cup sliced pepperoncini peppers"
        },
        {
            "5": "1 cup chopped red onion"
        },
        {
            "6": "0.5 cup chopped fresh cilantro"
        },
        {
            "7": "2 cups shredded Colby-Jack cheese"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 350 degrees F (175 degrees C)."
        },
        {
            "2": "Place pizza crust on a baking sheet. Spread barbeque sauce on crust. Top with chicken, pepperoncini peppers, onion, and cilantro. Cover with Colby-Jack cheese."
        },
        {
            "3": "Bake in the preheated oven until cheese is melted and bubbly, about 15 minutes."
        }
    ],

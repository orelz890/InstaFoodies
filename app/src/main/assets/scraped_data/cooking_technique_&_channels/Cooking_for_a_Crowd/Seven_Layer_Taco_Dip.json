{
    "title": "Seven-Layer Taco Dip",
    "Category": "Cooking for a Crowd",
    "Ingredients": [
        {
            "1": "1 (16 ounce) can refried beans"
        },
        {
            "2": "1 (1 ounce) package taco seasoning mix"
        },
        {
            "3": "1 (16 ounce) container sour cream"
        },
        {
            "4": "1 (8 ounce) package cream cheese, softened"
        },
        {
            "5": "1 (16 ounce) jar salsa"
        },
        {
            "6": "1 large tomato, chopped"
        },
        {
            "7": "1 medium green bell pepper, chopped"
        },
        {
            "8": "1 bunch chopped green onions"
        },
        {
            "9": "1 small head iceberg lettuce, shredded"
        },
        {
            "10": "2 cups shredded Cheddar cheese"
        },
        {
            "11": "1 (6 ounce) can sliced black olives, drained"
        }
    ],
    "Directions": [
        {
            "1": "Blend refried beans and taco seasoning in a medium bowl; spread mixture onto a large serving platter."
        },
        {
            "2": "Mix sour cream and cream cheese in a medium bowl; spread over refried beans."
        },
        {
            "3": "Top with salsa. Layer tomato, bell pepper, green onions, and lettuce over top."
        },
        {
            "4": "Sprinkle over Cheddar cheese and garnish with black olives."
        }
    ],
    "Details": [

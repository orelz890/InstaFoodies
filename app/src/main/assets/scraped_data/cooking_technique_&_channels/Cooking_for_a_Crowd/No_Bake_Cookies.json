{
    "title": "No-Bake Cookies",
    "Category": "Cooking for a Crowd",
    "Ingredients": [
        {
            "1": "0.75 cups white sugar"
        },
        {
            "2": "0.5 cup butter"
        },
        {
            "3": "0.5 cup milk"
        },
        {
            "4": "4 tablespoons unsweetened cocoa powder"
        },
        {
            "5": "0.5 cup crunchy peanut butter"
        },
        {
            "6": "3 cups quick-cooking oats"
        },
        {
            "7": "1 teaspoon vanilla extract"
        },
        {
            "8": "1 waxed paper"
        }
    ],
    "Directions": [
        {
            "1": "Combine sugar, butter, milk, and cocoa in a medium saucepan; bring to a boil, stirring often. Boil for exactly 1 1/2 minutes. Remove from heat, and stir in peanut butter. Add oats and vanilla; stir until well combined."
        },
        {
            "2": "Drop by teaspoonfuls onto waxed paper. Let cool until hardened."
        }
    ],

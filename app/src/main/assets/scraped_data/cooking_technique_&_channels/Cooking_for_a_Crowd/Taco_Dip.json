{
    "title": "Taco Dip",
    "Category": "Cooking for a Crowd",
    "Ingredients": [
        {
            "1": "1 (16 ounce) container nonfat sour cream"
        },
        {
            "2": "1 (8 ounce) package cream cheese, softened"
        },
        {
            "3": "1 (1.25 ounce) package taco seasoning mix"
        },
        {
            "4": "0.25 head iceberg lettuce - rinsed, dried, and shredded"
        },
        {
            "5": "1 cup shredded Cheddar cheese"
        },
        {
            "6": "3 medium chopped tomatoes"
        },
        {
            "7": "1 medium green bell pepper, chopped"
        },
        {
            "8": "1 (2.25 ounce) can black olives, drained"
        }
    ],
    "Directions": [
        {
            "1": "Mix sour cream, cream cheese, and taco seasoning together in a medium bowl until smooth. Spread mixture in a 9- or 10-inch round serving dish."
        },
        {
            "2": "Top with shredded lettuce, then sprinkle Cheddar cheese, tomatoes, bell pepper, and black olives on top."
        }
    ],

{
    "title": "Pumpkin Fluff Dip",
    "Category": "Cooking for a Crowd",
    "Ingredients": [
        {
            "1": "1 (15 ounce) can solid pack pumpkin"
        },
        {
            "2": "1 (5 ounce) package instant vanilla pudding mix"
        },
        {
            "3": "1 teaspoon pumpkin pie spice"
        },
        {
            "4": "1 (16 ounce) container frozen whipped topping, thawed"
        }
    ],
    "Directions": [
        {
            "1": "Mix together pumpkin, pudding mix, and pumpkin pie spice in a large bowl. Fold in whipped topping."
        },
        {
            "2": "Chill in the refrigerator until ready to serve."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },

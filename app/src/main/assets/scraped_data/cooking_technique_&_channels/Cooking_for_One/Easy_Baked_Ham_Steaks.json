{
    "title": "Easy Baked Ham Steaks",
    "Category": "Cooking for One",
    "Ingredients": [
        {
            "1": "1 cup water, or more as needed"
        },
        {
            "2": "3 tablespoons brown sugar"
        },
        {
            "3": "2 tablespoons Worcestershire sauce"
        },
        {
            "4": "5  whole cloves"
        },
        {
            "5": "2 slices ham steak"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the oven to 375 degrees F (190 degrees C)."
        },
        {
            "2": "Mix water, brown sugar, Worcestershire sauce, and cloves in a baking dish. Arrange ham steaks in baking dish, coating with sugar mixture. Add more water as needed to cover ham."
        },
        {
            "3": "Bake in the preheated oven until ham is tender, 35 to 45 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },

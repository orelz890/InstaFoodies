{
    "title": "Scrambled Eggs with Chorizo",
    "Category": "Cooking for One",
    "Ingredients": [
        {
            "1": "1 cooking spray"
        },
        {
            "2": "0.25 cup Mexican-style chorizo, or more to taste"
        },
        {
            "3": "6  eggs"
        },
        {
            "4": "1 salt to taste"
        }
    ],
    "Directions": [
        {
            "1": "Spray a large nonstick skillet with cooking spray; warm over medium-high heat. Add chorizo; cook and stir until browned, about 5 minutes."
        },
        {
            "2": "Whisk eggs and salt together in a bowl. Pour into the skillet; cook and stir until eggs are set, about 5 minutes."
        }
    ],

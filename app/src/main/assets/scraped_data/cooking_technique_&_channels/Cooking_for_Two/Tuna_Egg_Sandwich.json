{
    "title": "Tuna Egg Sandwich",
    "Category": "Cooking for Two",
    "Ingredients": [
        {
            "1": "3  hard-cooked eggs, peeled and chopped"
        },
        {
            "2": "1 (5 ounce) can tuna, drained"
        },
        {
            "3": "1 cup chopped celery"
        },
        {
            "4": "1 tablespoon mayonnaise"
        },
        {
            "5": "1 salt and pepper to taste"
        },
        {
            "6": "4 slices whole wheat bread"
        }
    ],
    "Directions": [
        {
            "1": "Combine eggs, tuna, celery, and mayonnaise in a medium bowl. Season with salt and pepper to taste."
        },
        {
            "2": "Divide tuna mixture evenly onto two slices of bread; top with remaining slices of bread."
        }
    ],

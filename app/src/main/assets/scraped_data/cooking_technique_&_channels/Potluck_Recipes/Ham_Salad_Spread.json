{
    "title": "Ham Salad Spread",
    "Category": "Potluck Recipes",
    "Ingredients": [
        {
            "1": "3 cups ground fully cooked ham"
        },
        {
            "2": "2  hard cooked eggs, chopped"
        },
        {
            "3": "2 tablespoons finely chopped celery"
        },
        {
            "4": "4 teaspoons sweet pickle relish"
        },
        {
            "5": "2 teaspoons finely chopped onion"
        },
        {
            "6": "1 cup mayonnaise"
        },
        {
            "7": "1 tablespoon prepared yellow mustard"
        }
    ],
    "Directions": [
        {
            "1": "Mix ham, eggs, celery, pickle relish, and onion together in a medium bowl until combined."
        },
        {
            "2": "Mix mayonnaise and mustard together in a small bowl; pour over ham mixture and stir to coat."
        },
        {
            "3": "Refrigerate until ready to serve."
        }
    ],
    "Details": [

{
    "title": "Easy Air Fryer Pork Chops",
    "Category": "Air Fryer Recipes",
    "Ingredients": [
        {
            "1": "0.5 cup grated Parmesan cheese"
        },
        {
            "2": "1 teaspoon paprika"
        },
        {
            "3": "1 teaspoon garlic powder"
        },
        {
            "4": "1 teaspoon kosher salt"
        },
        {
            "5": "1 teaspoon dried parsley"
        },
        {
            "6": "0.5 teaspoon ground black pepper"
        },
        {
            "7": "4 (5 ounce) boneless pork chops"
        },
        {
            "8": "2 tablespoons extra virgin olive oil"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the air fryer to 380 degrees F (190 degrees C)."
        },
        {
            "2": "Combine Parmesan cheese, paprika, garlic powder, salt, parsley, and pepper in a flat shallow dish; mix well."
        },
        {
            "3": "Coat each pork chop with olive oil. Dredge both sides of each chop in the Parmesan mixture and set on a plate."
        },
        {
            "4": "Place 2 chops in the basket of the air fryer and cook for 10 minutes; flipping halfway through cook time."
        },
        {
            "5": "Transfer to a cutting board and let rest for 5 minutes. Repeat with remaining chops."
        }
    ],
    "Details": [
        {
            "Prep Time": "5 mins"
        },
        {

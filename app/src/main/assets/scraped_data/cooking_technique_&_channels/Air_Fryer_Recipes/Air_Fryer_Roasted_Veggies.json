{
    "title": "Air-Fryer Roasted Veggies",
    "Category": "Air Fryer Recipes",
    "Ingredients": [
        {
            "1": "0.5 cup diced zucchini"
        },
        {
            "2": "0.5 cup diced summer squash"
        },
        {
            "3": "0.5 cup diced mushrooms"
        },
        {
            "4": "0.5 cup diced cauliflower"
        },
        {
            "5": "0.5 cup diced asparagus"
        },
        {
            "6": "0.5 cup diced sweet red pepper"
        },
        {
            "7": "2 teaspoons vegetable oil"
        },
        {
            "8": "0.25 teaspoon salt"
        },
        {
            "9": "0.25 teaspoon ground black pepper"
        },
        {
            "10": "0.25 teaspoon seasoning, or more to taste"
        }
    ],
    "Directions": [
        {
            "1": "Preheat the air fryer to 360 degrees F (180 degrees C)."
        },
        {
            "2": "Add zucchini, squash, mushrooms, cauliflower, asparagus, red pepper, oil, salt, pepper, and desired seasoning to a large bowl. Toss to coat; arrange in a single layer in the fryer basket."
        },
        {
            "3": "Cook, stirring halfway, in the preheated air fryer until tender crisp, about 10 minutes."
        }
    ],

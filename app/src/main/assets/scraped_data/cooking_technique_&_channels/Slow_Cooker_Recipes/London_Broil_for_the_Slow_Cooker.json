{
    "title": "London Broil for the Slow Cooker",
    "Category": "Slow Cooker Recipes",
    "Ingredients": [
        {
            "1": "0.5 pounds London broil"
        },
        {
            "2": "2 cloves garlic, minced"
        },
        {
            "3": "1 (10.5 ounce) can condensed cream of mushroom soup"
        },
        {
            "4": "0.5 cup water"
        },
        {
            "5": "0.5 teaspoon dried basil"
        },
        {
            "6": "0.5 teaspoon dried oregano"
        },
        {
            "7": "0.5 teaspoon salt"
        }
    ],
    "Directions": [
        {
            "1": "Place London broil in a slow cooker; add the garlic, condensed soup, water, basil, oregano, and salt; cover."
        },
        {
            "2": "Cook on Low 6 to 7 hours."
        }
    ],

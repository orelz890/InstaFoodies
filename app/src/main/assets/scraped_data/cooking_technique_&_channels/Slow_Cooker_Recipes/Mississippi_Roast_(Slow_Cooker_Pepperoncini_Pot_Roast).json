{
    "title": "Mississippi Roast (Slow Cooker Pepperoncini Pot Roast)",
    "Category": "Slow Cooker Recipes",
    "Ingredients": [
        {
            "1": "1 (4 pound) beef chuck roast"
        },
        {
            "2": "0.25 cup butter"
        },
        {
            "3": "5  pepperoncini peppers"
        },
        {
            "4": "1 (1 ounce) packet ranch dressing mix"
        },
        {
            "5": "1 (1 ounce) packet dry au jus mix"
        }
    ],
    "Directions": [
        {
            "1": "Place roast in a slow cooker. Form a pocket in the top of the roast and place butter, pepperoncini peppers, ranch dressing mix, and au jus mix in the pocket."
        },
        {
            "2": "Cook on Low for 8 hours."
        }
    ],

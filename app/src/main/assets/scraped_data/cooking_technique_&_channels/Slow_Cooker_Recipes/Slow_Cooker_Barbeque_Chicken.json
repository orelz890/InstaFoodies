{
    "title": "Slow Cooker Barbeque Chicken",
    "Category": "Slow Cooker Recipes",
    "Ingredients": [
        {
            "1": "4  skinless, boneless chicken breast halves"
        },
        {
            "2": "1 (18 ounce) bottle barbeque sauce (such as Sweet Baby Ray's\u00ae)"
        },
        {
            "3": "0.25 cup distilled white vinegar"
        },
        {
            "4": "0.25 cup brown sugar"
        },
        {
            "5": "1 teaspoon garlic powder"
        },
        {
            "6": "0.5 teaspoon red pepper flakes"
        }
    ],
    "Directions": [
        {
            "1": "Place chicken breast halves in a slow cooker."
        },
        {
            "2": "Whisk together barbeque sauce, vinegar, brown sugar, garlic powder, and pepper flakes in a bowl until sugar dissolves; pour over chicken."
        },
        {
            "3": "Cook on Low for 4 to 6 hours."
        }
    ],

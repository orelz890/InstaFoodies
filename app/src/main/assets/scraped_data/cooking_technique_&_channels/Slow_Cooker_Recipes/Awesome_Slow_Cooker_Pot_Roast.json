{
    "title": "Awesome Slow Cooker Pot Roast",
    "Category": "Slow Cooker Recipes",
    "Ingredients": [
        {
            "1": "2 (10.5 ounce) cans condensed cream of mushroom soup"
        },
        {
            "2": "0.25 cups water"
        },
        {
            "3": "1 (1 ounce) package dry onion soup mix"
        },
        {
            "4": "2.5 pounds pot roast"
        }
    ],
    "Directions": [
        {
            "1": "Mix condensed soup, water, and dry onion soup mix in a slow cooker. Place pot roast in the slow cooker and coat with soup mixture."
        },
        {
            "2": "Cook on Low for 8 to 9 hours or High for 3 to 4 hours."
        }
    ],

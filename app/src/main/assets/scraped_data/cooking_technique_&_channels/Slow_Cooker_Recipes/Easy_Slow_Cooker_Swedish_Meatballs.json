{
    "title": "Easy Slow Cooker Swedish Meatballs",
    "Category": "Slow Cooker Recipes",
    "Ingredients": [
        {
            "1": "2 cups beef broth"
        },
        {
            "2": "1 (10.5 ounce) can condensed cream of mushroom soup"
        },
        {
            "3": "2 tablespoons steak sauce"
        },
        {
            "4": "1 tablespoon Worcestershire sauce"
        },
        {
            "5": "1 teaspoon garlic powder"
        },
        {
            "6": "0.5 teaspoon paprika"
        },
        {
            "7": "0.5 teaspoon salt"
        },
        {
            "8": "0.5 teaspoon ground black pepper"
        },
        {
            "9": "1 (28 ounce) package frozen cooked meatballs, thawed"
        },
        {
            "10": "1  onion, diced"
        },
        {
            "11": "1 cup sour cream"
        }
    ],
    "Directions": [
        {
            "1": "Whisk together beef broth and condensed soup in a slow cooker. Add steak sauce, Worcestershire sauce, garlic powder, paprika, salt, and pepper. Stir in meatballs and onion."
        },
        {
            "2": "Cook on High for 4 hours."
        },
        {
            "3": "Stir in sour cream. Continue cooking until sauce is heated through, about 30 minutes."
        }
    ],
    "Details": [
        {
            "Prep Time": "10 mins"
        },

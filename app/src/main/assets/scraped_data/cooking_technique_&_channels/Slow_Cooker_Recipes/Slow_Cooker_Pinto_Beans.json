{
    "title": "Slow Cooker Pinto Beans",
    "Category": "Slow Cooker Recipes",
    "Ingredients": [
        {
            "1": "1 pound dried pinto beans, soaked overnight"
        },
        {
            "2": "1 pound Black Forest ham, cubed"
        },
        {
            "3": "1 medium onion, chopped"
        },
        {
            "4": "0.75 medium green bell pepper, diced"
        },
        {
            "5": "2 medium celery ribs, chopped"
        },
        {
            "6": "3 cloves garlic, chopped"
        },
        {
            "7": "1 tablespoon dried oregano, or to taste"
        },
        {
            "8": "1 tablespoon lard"
        },
        {
            "9": "1 teaspoon ground cumin, or to taste"
        },
        {
            "10": "2 large bay leaves"
        },
        {
            "11": "5 cups chicken broth, or as needed to cover"
        }
    ],
    "Directions": [
        {
            "1": "Place soaked pinto beans in a slow cooker. Add ham, onion, bell pepper, celery, garlic, oregano, lard, cumin, and bay leaves. Pour in enough chicken broth to cover all ingredients."
        },
        {
            "2": "Cook on High until beans are very tender, 5 to 6 hours."
        }
    ],

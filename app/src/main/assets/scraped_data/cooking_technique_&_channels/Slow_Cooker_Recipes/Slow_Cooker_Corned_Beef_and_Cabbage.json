{
    "title": "Slow-Cooker Corned Beef and Cabbage",
    "Category": "Slow Cooker Recipes",
    "Ingredients": [
        {
            "1": "10  baby red potatoes, quartered"
        },
        {
            "2": "4 large carrots, peeled and cut into matchstick pieces"
        },
        {
            "3": "1  onion, peeled and cut into bite-sized pieces"
        },
        {
            "4": "4 cups water"
        },
        {
            "5": "1 (4 pound) corned beef brisket with spice packet"
        },
        {
            "6": "6 ounces beer"
        },
        {
            "7": "0.5 head cabbage, coarsely chopped"
        }
    ],
    "Directions": [
        {
            "1": "Place potatoes, carrots, and onion into the bottom of a slow cooker; add water and place brisket on top of vegetables. Pour beer over brisket; sprinkle over spices from the packet and cover."
        },
        {
            "2": "Cook on High for 7 hours; stir in the cabbage and cook for 1 more hour."
        }
    ],

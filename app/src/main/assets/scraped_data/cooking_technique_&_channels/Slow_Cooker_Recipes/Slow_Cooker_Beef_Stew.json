{
    "title": "Slow Cooker Beef Stew",
    "Category": "Slow Cooker Recipes",
    "Ingredients": [
        {
            "1": "2 pounds beef stew meat, cut into 1-inch pieces"
        },
        {
            "2": "0.25 cup all-purpose flour"
        },
        {
            "3": "0.5 teaspoon salt"
        },
        {
            "4": "0.5 teaspoon ground black pepper"
        },
        {
            "5": "0.5 cups beef broth"
        },
        {
            "6": "4 medium carrots, sliced"
        },
        {
            "7": "3 medium potatoes, diced"
        },
        {
            "8": "1 medium onion, chopped"
        },
        {
            "9": "1 stalk celery, chopped"
        },
        {
            "10": "1 teaspoon Worcestershire sauce"
        },
        {
            "11": "1 teaspoon ground paprika"
        },
        {
            "12": "1 clove garlic, minced"
        },
        {
            "13": "1 large bay leaf"
        }
    ],
    "Directions": [
        {
            "1": "Place meat in slow cooker."
        },
        {
            "2": "Mix flour, salt, and pepper together in a small bowl. Pour over meat, and stir until meat is coated."
        },
        {
            "3": "Add beef broth, carrots, potatoes, onion, celery, Worcestershire sauce, paprika, garlic, and bay leave; stir to combine."
        },
        {
            "4": "Cover, and cook until beef is tender enough to cut with a spoon, on Low for 8 to 12 hours, or on High for 4 to 6 hours."
        }
    ],
    "Details": [
        {
            "Prep Time": "20 mins"
        },

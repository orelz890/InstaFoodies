{
    "title": "Slow Cooker Sausage with Sauce",
    "Category": "Slow Cooker Recipes",
    "Ingredients": [
        {
            "1": "8 (4 ounce) links fresh Italian sausage"
        },
        {
            "2": "1 (26 ounce) jar spaghetti sauce"
        },
        {
            "3": "1  green bell pepper, seeded and sliced into strips"
        },
        {
            "4": "1  onion, sliced"
        },
        {
            "5": "6  hoagie rolls, split lengthwise (Optional)"
        }
    ],
    "Directions": [
        {
            "1": "Place Italian sausage links, spaghetti sauce, green pepper, and onion into a slow cooker; mix until well combined."
        },
        {
            "2": "Cover and cook on Low for 6 hours. Serve on hoagie rolls."
        }
    ],

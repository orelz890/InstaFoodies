{
    "title": "Slow Cooker Creamed Corn",
    "Category": "Slow Cooker Recipes",
    "Ingredients": [
        {
            "1": "0.25 (16 ounce) packages frozen corn kernels"
        },
        {
            "2": "1 (8 ounce) package cream cheese"
        },
        {
            "3": "0.5 cup butter"
        },
        {
            "4": "0.5 cup milk"
        },
        {
            "5": "1 tablespoon white sugar"
        },
        {
            "6": "1 salt and pepper to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine corn, cream cheese, butter, milk, and sugar in a slow cooker. Season with salt and pepper to taste."
        },
        {
            "2": "Cook on Low for 4 to 6 hours or High for 2 to 4 hours."
        }
    ],

{
    "title": "Zesty Slow Cooker Chicken Barbecue",
    "Category": "Slow Cooker Recipes",
    "Ingredients": [
        {
            "1": "6  frozen skinless, boneless chicken breast halves"
        },
        {
            "2": "1 (12 ounce) bottle barbeque sauce"
        },
        {
            "3": "0.5 cup Italian salad dressing"
        },
        {
            "4": "0.25 cup brown sugar"
        },
        {
            "5": "2 tablespoons Worcestershire sauce"
        }
    ],
    "Directions": [
        {
            "1": "Place chicken in the slow cooker. Mix barbecue sauce, Italian salad dressing, brown sugar, and Worcestershire sauce in a bowl; pour mixture over chicken."
        },
        {
            "2": "Cover and cook on Low for 6 to 8 hours or on High for 3 to 4 hours. Shred and serve."
        }
    ],

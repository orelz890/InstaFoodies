{
    "title": "Peppermint Coffee Creamer",
    "Category": "Allrecipes Allstars",
    "Ingredients": [
        {
            "1": "0.75 cup milk"
        },
        {
            "2": "0.25 cup heavy whipping cream"
        },
        {
            "3": "2 tablespoons stevia powder, or to taste"
        },
        {
            "4": "1 teaspoon vanilla extract"
        },
        {
            "5": "0.5 teaspoon peppermint extract, or more to taste"
        }
    ],
    "Directions": [
        {
            "1": "Combine milk, whipping cream, and stevia in a saucepan over medium-low heat. Heat until bubbles start to appear on the outer edges of the saucepan, 5 to 7 minutes; do not boil. Remove from the heat and stir in vanilla and peppermint extracts."
        }
    ],

{
    "title": "Raspberry-Honey Shmear",
    "Category": "Allrecipes Allstars",
    "Ingredients": [
        {
            "1": "1 (8 ounce) package cream cheese, cut into chunks"
        },
        {
            "2": "3 ounces fresh raspberries, rinsed and patted dry"
        },
        {
            "3": "2 tablespoons honey"
        }
    ],
    "Directions": [
        {
            "1": "Place cream cheese in a bowl and microwave at 30% power in 10 second intervals until slightly soft but not warm, approximately 20 to 30 seconds."
        },
        {
            "2": "Place raspberries and honey over top and lightly mash raspberries with a fork. Stir mixture until evenly combined."
        }
    ],
